{
  "name": "@relay/ai-agent",
  "version": "1.0.0",
  "description": "Relay AI Agent System - Dual-agent privacy-first development environment",
  "type": "module",
  "main": "aiRelayAgent.mjs",
  "bin": {
    "relay-ai-agent": "./aiAgentCLI.mjs"
  },
  "scripts": {
    "start": "node aiRelayAgent.mjs",
    "demo": "node aiRelayAgentDemo.mjs",
    "demo:interactive": "node aiRelayAgentDemo.mjs interactive",
    "demo:privacy": "node aiRelayAgentDemo.mjs privacy", 
    "demo:models": "node aiRelayAgentDemo.mjs models",
    "test": "vitest run agentTestSuite.mjs",
    "test:watch": "vitest agentTestSuite.mjs",
    "test:coverage": "vitest run --coverage agentTestSuite.mjs",
    "cli": "node aiAgentCLI.mjs",
    "cli:chat": "node aiAgentCLI.mjs chat",
    "cli:status": "node aiAgentCLI.mjs system status",
    "cli:health": "node aiAgentCLI.mjs system health",
    "setup": "node aiAgentCLI.mjs config setup",
    "build:index": "node aiAgentCLI.mjs index build",
    "validate": "node aiAgentCLI.mjs config validate"
  },
  "dependencies": {
    "openai": "^4.28.0",
    "@anthropic-ai/sdk": "^0.15.0",
    "node-fetch": "^3.3.2",
    "crypto": "^1.0.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12"
  },
  "devDependencies": {
    "vitest": "^1.2.0",
    "@vitest/coverage-c8": "^0.33.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0"
  },
  "peerDependencies": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "agent",
    "gpt",
    "claude",
    "privacy",
    "relay",
    "dual-agent",
    "llm",
    "collaboration"
  ],
  "author": "Relay Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/relay/relay-codebase.git",
    "directory": "src/backend/ai-agent"
  },
  "bugs": {
    "url": "https://github.com/relay/relay-codebase/issues"
  },
  "homepage": "https://github.com/relay/relay-codebase#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "*.mjs",
    "*.json",
    "README.md"
  ],
  "preferGlobal": true
}
