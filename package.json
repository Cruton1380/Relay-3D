{
  "name": "@relay-network/sdk",
  "version": "1.0.0",
  "description": "Relay Network SDK - Decentralized Consensus and Privacy for Modern Applications",
  "main": "relayNetworkSDK.mjs",
  "type": "module",
  "license": "MIT",
  "author": "Relay Network Contributors",
  "homepage": "https://relay.network",
  "keywords": [
    "consensus",
    "privacy",
    "zero-knowledge",
    "governance",
    "blockchain",
    "p2p",
    "decentralized",
    "voting",
    "hashgraph",
    "zk-stark"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:fast": "vitest run --no-coverage --reporter=default",
    "test:parallel": "vitest run --maxWorkers=4",
    "test:single": "vitest run --maxWorkers=1 --no-coverage",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:coverage": "vitest run --coverage",
    "maintenance:reset": "node scripts/system-maintenance.mjs reset",
    "maintenance:clear-cache": "node scripts/system-maintenance.mjs clear-cache",
    "maintenance:validate": "node scripts/system-maintenance.mjs validate",
    "maintenance:fix": "node scripts/system-maintenance.mjs fix",
    "maintenance:backup": "node scripts/system-maintenance.mjs backup",
    "dev:enhanced": "node src/backend/server-enhanced.mjs",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:fix": "npm audit fix",
    "security:audit:force": "npm audit fix --force",
    "security:check": "node scripts/security-check.mjs",
    "backup:data": "node scripts/backup-data.mjs",
    "test:threshold": "vitest run --coverage.branches=70 --coverage.functions=75 --coverage.lines=80 --coverage.statements=80",
    "test:ui": "vitest --ui",
    "test:backend": "vitest run \"test/backend/**/*.test.{js,mjs}\"",
    "test:frontend": "vitest run \"test/frontend/**/*.test.{js,jsx}\"",
    "test:integration": "vitest run \"test/integration/**/*.test.{js,mjs,jsx}\"",
    "test:e2e": "playwright test",
    "test:e2e-api": "vitest run test/integration/endToEndApi.test.mjs",
    "test:ci": "vitest run --coverage && npm run lint",
    "test:crypto": "vitest run \"test/x25519-*.test.{js,mjs}\"",
    "test:abuse": "vitest run test/x25519-abuse-testing.test.mjs",
    "test:advanced": "vitest run test/advanced-features.test.mjs",
    "test:advanced:watch": "vitest test/advanced-features.test.mjs",
    "test:performance": "vitest run tests/performance/ --no-coverage",
    "test:unit": "vitest run tests/unit/ --no-coverage",
    "test:optimize": "npm run clean:cache && npm run test:fast",
    "integration:backend": "vitest run test/integration/backendIntegration.test.mjs",
    "integration:frontend": "vitest run test/integration/frontendIntegration.test.jsx",
    "integration:api": "vitest run test/integration/endToEndApi.test.mjs",
    "playwright:install": "playwright install --with-deps",
    "lint": "eslint . --ext .js,.jsx,.mjs",
    "security:audit-prod": "npm audit --audit-level=high --production",
    "security:fix": "npm audit fix",
    "security:check-stablelib": "npm ls @stablelib/x25519 @stablelib/random @stablelib/base64",
    "cleanup:node": "powershell -ExecutionPolicy Bypass -File scripts/kill-all-node.ps1",
    "start": "concurrently --kill-others --prefix-colors \"bgBlue.bold,bgGreen.bold\" --names \"BACKEND,FRONTEND\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "start:reliable": "echo 'Start backend: npm run dev:backend, then in new terminal: npm run dev:frontend'",
    "start:complete": "node scripts/start-complete-system.mjs",
    "cleanup:ports": "node scripts/cleanup-ports.mjs",
    "start:system": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "start:production": "node src/backend/server.mjs",
    "start:dev": "echo 'Use: npm run dev:backend + npm run dev:frontend'",
    "start:legacy": "npm run start:system",
    "dev": "echo 'Use: npm run dev:backend (wait for ready) then npm run dev:frontend'",
    "relay": "node tools/relay-launcher.mjs",
    "dev:backend": "node scripts/cleanup-before-start.mjs backend && nodemon src/backend/server.mjs",
    "dev:frontend": "node scripts/cleanup-before-start.mjs frontend && vite --port 5176",
    "dev:cesium": "node scripts/dev-server.mjs",
    "dev:globe-services": "node scripts/globe-services-server.mjs",
    "verify:entry": "node scripts/verify-entry.mjs",
    "verify:prompt": "node scripts/verify-prompt.mjs",
    "verify:integrity": "node scripts/verify-integrity.mjs",
    "gate:slice": "node scripts/slice-gate.mjs",
    "lint:forbidden-language": "node scripts/forbidden-language-lint.mjs",
    "dev:both": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "boot-gate": "node scripts/boot-gate-test.mjs",
    "download:counties": "node scripts/download-all-counties.mjs",
    "load:mock-voters": "node scripts/load-mock-voters-via-api.mjs",
    "generate-demo-voters": "node scripts/generate-voters-with-locations.mjs && node scripts/load-demo-voters.mjs",
    "build:frontend": "vite build",
    "build:frontend:standalone": "node scripts/build-frontend.mjs",
    "build:backend:standalone": "node scripts/build-backend.mjs",
    "build:all": "npm run build:frontend:standalone && npm run build:backend:standalone",
    "preview:frontend": "vite preview",
    "services:channel": "node src/backend/channel-service/index.mjs",
    "services:social": "node src/backend/social-service/index.mjs",
    "services:hardware": "node src/backend/hardware-scanning-service/index.mjs",
    "services:all": "node scripts/cleanup-ports.mjs && node scripts/start-all-services.mjs",
    "services:dev": "nodemon scripts/start-all-services.mjs",
    "verify": "node scripts/system-health-check.mjs",
    "health": "node scripts/system-health-check.mjs",
    "performance:monitor": "node scripts/performance-management.mjs monitor",
    "performance:optimize": "node scripts/performance-management.mjs optimize",
    "performance:full": "node scripts/performance-management.mjs full",
    "clean:logs": "node -e \"const fs=require('fs'); fs.readdirSync('./logs').filter(f=>!f.includes(new Date().toISOString().split('T')[0])).forEach(f=>fs.unlinkSync('./logs/'+f))\"",
    "clean:chrome": "powershell -ExecutionPolicy Bypass -File scripts/clear-chrome-cache.ps1",
    "test:browser": "powershell -ExecutionPolicy Bypass -File scripts/test-browser-connectivity.ps1",
    "optimize:vscode": "echo 'VS Code optimizations applied! Restart VS Code for best performance.'"
  },
