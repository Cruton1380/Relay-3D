{
  "navigator": {
    "defaultModel": "gpt-4o",
    "defaultSource": "cloud",
    "fallbackModels": [
      { "model": "gpt-4-turbo", "source": "cloud" },
      { "model": "llama-3.1-8b", "source": "local" },
      { "model": "mistral-7b", "source": "local" }
    ],
    "capabilities": ["ui_guidance", "governance_explanation", "keyspace_management"]
  },
  "architect": {
    "defaultModel": "claude-3.5-sonnet",
    "defaultSource": "cloud",
    "fallbackModels": [
      { "model": "claude-3-haiku", "source": "cloud" },
      { "model": "deepseek-coder", "source": "user" },
      { "model": "llama-3.1-70b", "source": "local" },
      { "model": "codellama-13b", "source": "local" }
    ],
    "capabilities": ["code_analysis", "implementation", "architecture_design"]
  },
  "sourceTypes": {
    "cloud": {
      "description": "Official cloud APIs (OpenAI, Anthropic)",
      "requiresInternet": true,
      "requiresApiKey": true,
      "costPerToken": "variable",
      "privacy": "data_sent_to_provider"
    },
    "local": {
      "description": "Self-hosted models (Ollama, vLLM, llama.cpp)",
      "requiresInternet": false,
      "requiresApiKey": false,
      "costPerToken": "free",
      "privacy": "data_stays_local"
    },
    "user": {
      "description": "User-provided API endpoints and keys",
      "requiresInternet": true,
      "requiresApiKey": true,
      "costPerToken": "user_account",
      "privacy": "user_controlled"
    }
  },
  "availableModels": {
    "cloud": {
      "openai": ["gpt-4o", "gpt-4-turbo", "gpt-3.5-turbo"],
      "anthropic": ["claude-3.5-sonnet", "claude-3-haiku", "claude-3-opus"],
      "google": ["gemini-pro", "gemini-pro-vision"]
    },
    "local": {
      "ollama": ["llama-3.1-8b", "llama-3.1-70b", "mistral-7b", "codellama-13b", "deepseek-coder"],
      "vllm": ["llama-3.1-8b", "mistral-7b", "codellama-13b"],
      "llamacpp": ["llama-3.1-8b", "mistral-7b", "qwen-14b"]
    },
    "user": {
      "deepseek": ["deepseek-coder", "deepseek-chat"],
      "together": ["llama-3.1-70b", "mixtral-8x7b"],
      "replicate": ["llama-3.1-405b", "codellama-70b"]
    }
  },
  "modelCapabilities": {
    "gpt-4o": {
      "contextWindow": 128000,
      "strengths": ["reasoning", "UI guidance", "governance"],
      "provider": "openai",
      "sourceType": "cloud",
      "endpoint": "https://api.openai.com/v1/chat/completions",
      "costTier": "premium"
    },
    "claude-3.5-sonnet": {
      "contextWindow": 200000,
      "strengths": ["code analysis", "architecture", "implementation"],
      "provider": "anthropic",
      "sourceType": "cloud",
      "endpoint": "https://api.anthropic.com/v1/messages",
      "costTier": "premium"
    },
    "deepseek-coder": {
      "contextWindow": 16000,
      "strengths": ["code generation", "debugging"],
      "provider": "deepseek",
      "sourceType": "user",
      "endpoint": "https://api.deepseek.com/chat/completions",
      "costTier": "affordable"
    },
    "llama-3.1-8b": {
      "contextWindow": 8000,
      "strengths": ["general purpose", "local inference"],
      "provider": "meta",
      "sourceType": "local",
      "endpoint": "http://localhost:11434/api/generate",
      "costTier": "free"
    },
    "llama-3.1-70b": {
      "contextWindow": 8000,
      "strengths": ["advanced reasoning", "code analysis"],
      "provider": "meta",
      "sourceType": "local",
      "endpoint": "http://localhost:11434/api/generate",
      "costTier": "free"
    },
    "mistral-7b": {
      "contextWindow": 8000,
      "strengths": ["efficiency", "local inference"],
      "provider": "mistral",
      "sourceType": "local",
      "endpoint": "http://localhost:11434/api/generate",
      "costTier": "free"
    },
    "codellama-13b": {
      "contextWindow": 16000,
      "strengths": ["code generation", "code completion"],
      "provider": "meta",
      "sourceType": "local",
      "endpoint": "http://localhost:11434/api/generate",
      "costTier": "free"
    }
  },
  "endpointConfiguration": {
    "cloud": {
      "openai": {
        "baseUrl": "https://api.openai.com/v1",
        "authHeader": "Authorization: Bearer {api_key}",
        "model_param": "model",
        "timeout": 60000
      },
      "anthropic": {
        "baseUrl": "https://api.anthropic.com/v1",
        "authHeader": "x-api-key: {api_key}",
        "model_param": "model",
        "timeout": 60000
      }
    },
    "local": {
      "ollama": {
        "baseUrl": "http://localhost:11434/api",
        "healthEndpoint": "/tags",
        "generateEndpoint": "/generate",
        "timeout": 120000
      },
      "vllm": {
        "baseUrl": "http://localhost:8000/v1",
        "healthEndpoint": "/models",
        "generateEndpoint": "/chat/completions",
        "timeout": 120000
      },
      "llamacpp": {
        "baseUrl": "http://localhost:8080",
        "healthEndpoint": "/health",
        "generateEndpoint": "/completion",
        "timeout": 120000
      }
    },
    "user": {
      "deepseek": {
        "baseUrl": "https://api.deepseek.com",
        "authHeader": "Authorization: Bearer {api_key}",
        "model_param": "model",
        "timeout": 60000
      },
      "together": {
        "baseUrl": "https://api.together.xyz/v1",
        "authHeader": "Authorization: Bearer {api_key}",
        "model_param": "model",
        "timeout": 60000
      }
    }
  },
  "availabilityChecks": {
    "cloud": {
      "interval": 300000,
      "healthEndpoints": {
        "openai": "https://api.openai.com/v1/models",
        "anthropic": "https://api.anthropic.com/v1/messages",
        "google": "https://generativelanguage.googleapis.com/v1/models"
      }
    },
    "local": {
      "interval": 60000,
      "healthEndpoints": {
        "ollama": "http://localhost:11434/api/tags",
        "vllm": "http://localhost:8000/v1/models",
        "llamacpp": "http://localhost:8080/health"
      }
    },
    "user": {
      "interval": 300000,
      "requiresValidation": true
    }
  }
}
