{
  "invitePrivacy": {
    "temporalMixing": {
      "enabled": true,
      "batchWindow": "1h",
      "batchWindowMs": 3600000,
      "randomizationRange": 0.8,
      "description": "Time window for batching invites. Options: 1h, 6h, 24h"
    },
    "phantomInvites": {
      "enabled": true,
      "phantomRatio": 0.15,
      "minPhantomRatio": 0.05,
      "maxPhantomRatio": 0.2,
      "phantomPrefix": "ph_",
      "communityConfigurable": true,
      "description": "Ratio of phantom invites to real invites (community-votable parameter)"
    },
    "security": {
      "auditLogging": true,
      "internalMetrics": true,
      "rollbackEnabled": true,
      "emergencyDisable": false,
      "description": "Security and audit configuration"
    },
    "governance": {
      "communityVotableParams": [
        "phantomRatio",
        "batchWindow",
        "enabled"
      ],
      "lastCommunityVote": null,
      "nextVoteEligible": null,
      "description": "Parameters that can be modified via community voting"
    },
    "compatibility": {
      "backwardCompatible": true,
      "preserveExistingInvites": true,
      "gracefulDegradation": true,
      "description": "Ensures no breaking changes to existing system"
    },
    "phantomRatio": 0.15
  }
}